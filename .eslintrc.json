{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"linebreak-style": 0,
		"no-console": 0,
		"indent": 0,
		"react/jsx-closing-bracket-location": 1,
		"react/jsx-indent-props": 0,
		"react/jsx-indent": 0
	}
}
